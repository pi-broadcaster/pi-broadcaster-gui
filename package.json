{
  "name": "pi-broadcaster",
  "version": "2.0.0",
  "description": "Pi Broadcaster Manager GUI",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pham-tuyen/pi-broadcaster.git"
  },
  "author": "Pham Tuyen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/pham-tuyen/pi-broadcaster/issues"
  },
  "homepage": "https://github.com/pham-tuyen/pi-broadcaster#readme",
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0"
  },
  "build": {
    "appId": "pi.broadcaster.manager",
    "productName": "Pi Broadcaster Manager",
    "win": {
      "target": {
        "target": "msi",
        "arch": "ia32"
      }
    }
  },
  "dependencies": {
    "simple-ssh": "^1.1.1",
    "ssh2-sftp-client": "^9.1.0"
  }
}
